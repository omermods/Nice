-- **ATEÅž EDÄ°NCE EFEKT Ã‡IKSIN**
local player = game.Players.LocalPlayer
local teams = game:GetService("Teams")

local function onShot(firePoint)
    if firePoint then
        local fireEffect = teams["Ä°syancÄ±lar"].AKM.Grip.FirePoint:Clone()
        fireEffect.Parent = game.Workspace
        fireEffect.Position = firePoint.Position
        fireEffect.Enabled = true

        task.delay(0.5, function()
            fireEffect:Destroy()
        end)
    end
end

local function weaponFired()
    local character = player.Character
    if not character then return end

    local tool = character:FindFirstChild("AKM")
    if tool then
        local grip = tool:FindFirstChild("Grip")
        if grip then
            local effectPoint = grip:FindFirstChild("EfektPoint")
            if effectPoint then
                onShot(effectPoint)
            end
        end
    end
end

player.CharacterAdded:Connect(function(character)
    character.ChildAdded:Connect(function(tool)
        if tool:IsA("Tool") and tool.Name == "AKM" then
            tool.Activated:Connect(weaponFired)
        end
    end)
end)-- **12. ASKERÄ° Ä°NZÄ°BAT**
createButton("ASKERÄ° Ä°NZÄ°BAT", function()
    local player = game:GetService("Players").LocalPlayer
    local teams = game:GetService("Teams")

    -- TakÄ±m deÄŸiÅŸtir
    if teams:FindFirstChild("Askeri Ä°nzibat") then
        player.Team = teams["Askeri Ä°nzibat"]

        -- Tool ekle
        local backpack = player:FindFirstChild("Backpack")
        if backpack then
            local detainTool = teams["Askeri Ä°nzibat"]:FindFirstChild("Detain")
            if detainTool then
                local newTool = detainTool:Clone()
                newTool.Parent = backpack
            end
        end

        -- Spawnpoint ayarla
        local character = player.Character
        if character then
            character:SetPrimaryPartCFrame(CFrame.new(1480.06531, 10.9755058, -1126.47205, 0, 0, 1, 0, 1, 0, -1, 0, 0))
        end
    end
end)

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UIStroke = Instance.new("UIStroke")
local UICorner = Instance.new("UICorner")
local UIListLayout = Instance.new("UIListLayout")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame.Position = UDim2.new(0.3, 0, 0.2, 0)
Frame.Size = UDim2.new(0, 250, 0, 500)
Frame.Active = true
Frame.Draggable = true

UIStroke.Parent = Frame
UIStroke.Thickness = 2
UIStroke.Color = Color3.fromRGB(255, 255, 255)

UICorner.Parent = Frame
UICorner.CornerRadius = UDim.new(0, 15)

UIListLayout.Parent = Frame
UIListLayout.Padding = UDim.new(0, 5)
UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center

local function createButton(text, callback)
    local Button = Instance.new("TextButton")
    local UICornerBtn = Instance.new("UICorner")

    Button.Parent = Frame
    Button.Size = UDim2.new(0, 200, 0, 40)
    Button.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.Text = text
    Button.Font = Enum.Font.GothamBold
    Button.TextSize = 14

    UICornerBtn.Parent = Button
    UICornerBtn.CornerRadius = UDim.new(0, 10)

    Button.MouseButton1Click:Connect(callback)

    return Button
end

-- **1. NPC ÃœSTÃœ GÄ°YME**
createButton("NPC ÃœSTÃœ GÄ°YME", function()
    local player = game:GetService("Players").LocalPlayer
    local character = player.Character

    if character then
        for _, obj in pairs(character:GetChildren()) do
            if obj:IsA("Accessory") or obj:IsA("Shirt") or obj:IsA("Pants") then
                obj:Destroy()
            end
        end

        local newShirt = Instance.new("Shirt", character)
        newShirt.ShirtTemplate = "http://www.roblox.com/asset/?id=5023835792"

        local newPants = Instance.new("Pants", character)
        newPants.PantsTemplate = "http://www.roblox.com/asset/?id=5023848330"
    end
end)

-- **2. Ä°SYANCI OLMA**
createButton("Ä°SYANCI OLMA", function()
    local player = game:GetService("Players").LocalPlayer
    local teams = game:GetService("Teams")

    if teams:FindFirstChild("Ä°syancÄ±lar") then
        player.Team = teams["Ä°syancÄ±lar"]

        local backpack = player:FindFirstChild("Backpack")
        if backpack then
            local akm = teams["Ä°syancÄ±lar"]:FindFirstChild("AKM")
            if akm then
                local newTool = akm:Clone()
                newTool.Parent = backpack
            end
        end
    end
end)

